{
    "name": "f7media/cacheflow",
    "type": "typo3-cms-extension",
    "description": "Continuous background refreshing of cached pages.",
    "homepage": "https://github.com/f7media/cacheflow",
    "support": {
        "source": "https://github.com/f7media/cacheflow",
        "issues": "https://github.com/f7media/cacheflow/issues",
        "docs": "https://docs.typo3.org/p/f7media/cacheflow/main/en-us/"
    },
    "authors": [
        {
            "name": "David Nax",
            "role": "Developer"
        },
        {
            "name": "Simon Pritzkat-Gerthenrich",
            "role": "Project Manager"
        },
        {
            "name": "Marc Willmann",
            "role": "Supervisor"
        }
    ],
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "typo3/cms-core": "^12.4 || ^13.4",
        "typo3/cms-backend": "^12.4 || ^13.4",
        "typo3/cms-dashboard": "^12.4 || ^13.4"
    },
    "autoload": {
        "psr-4": {
            "F7media\\Cacheflow\\": "Classes/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    },
    "require-dev": {
        "bnf/phpstan-psr-container": "^1.0.1",
        "ext-zip": "*",
        "friendsofphp/php-cs-fixer": "^3.22",
        "friendsoftypo3/phpstan-typo3": "^0.9.0",
        "phpstan/phpstan": "^1.10.22",
        "phpstan/phpstan-phpunit": "^1.3.13",
        "typo3/coding-standards": "^0.8",
        "typo3/testing-framework": "^8.2",
        "typo3/tailor": "^1.6"
    },
    "autoload-dev": {
        "psr-4": {
            "F7media\\Cacheflow\\Tests\\": "Tests"
        }
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/Web",
            "app-dir": ".Build",
            "extension-key": "cacheflow"
        }
    }
}
