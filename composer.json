{
    "name": "f7media/cacheflow",
    "type": "typo3-cms-extension",
    "description": "Continuous background refreshing of cached pages.",
    "homepage": "https://github.com/f7media/cacheflow",
    "authors": [
        {
            "name": "David Nax",
            "role": "Developer"
        },
        {
            "name": "Simon Pritzkat-Gerthenrich",
            "role": "Project Manager"
        },
        {
            "name": "Marc Willmann",
            "role": "Supervisor"
        }
    ],
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "typo3/cms-core": "^12.4",
        "typo3/cms-backend": "^12.4",
        "typo3/cms-dashboard": "^12.4"
    },
    "autoload": {
        "psr-4": {
            "F7media\\Cacheflow\\": "Classes/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    },
    "require-dev": {
        "ext-zip": "*",
        "typo3/cms-dashboard": "^12.4",
        "typo3/testing-framework": "^8.2",
        "typo3/tailor": "^1.6"
    },
    "autoload-dev": {
        "psr-4": {
            "F7media\\Cacheflow\\Tests\\": "Tests"
        }
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/Web",
            "app-dir": ".Build",
            "extension-key": "cacheflow"
        }
    }
}
